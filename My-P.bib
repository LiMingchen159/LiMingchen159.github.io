@article{wangEvaluatingComfortThermally2020,
  title = {Evaluating the Comfort of Thermally Dynamic Wearable Devices},
  author = {Wang, Zhe and Warren, Kristen and Luo, Maohui and He, Xuchen and Zhang, Hui and Arens, Edward and Chen, Wenhua and He, Yingdon and Hu, Yunpeng and Jin, Ling and Liu, Shichao and {Cohen-Tanugi}, David and Smith, Matthew J.},

@article{queirozInteroperabilityAutodeskRevit2019,
  title = {Interoperability between {{Autodesk Revit}} and {{EnergyPlus}} for Thermal Simulations of Buildings},
  author = {Queir{\'o}z, Gabriel and Grigoletti, Giane and Santos, Joaquim},

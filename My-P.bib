@article{pudleinerUsingSensitivityAnalysis2015,
  title = {Using Sensitivity Analysis to Improve the Efficiency of a {{Net-Zero Energy}} Vaccine Warehouse Design},
  author = {Pudleiner, David and Colton, Jonathan},

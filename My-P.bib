@article{linczukMultiobjectiveOptimisationTargeted2020,
  title = {Multi-Objective Optimisation Targeted at the Thermal Performance of Low-Energy Buildings Design in Southern {{Brazil}}},
  author = {Linczuk, Vin{\'i}cius Cesar Cadena and Bastos, Leopoldo Eurico Gon{\c c}alves},
  year = {2020},
  month = oct,
  journal = {Ambiente Constru{\'i}do},

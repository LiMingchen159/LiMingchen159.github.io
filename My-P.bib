@article{guoOptimizedDesignFloor2022,
  title = {Optimized Design of Floor Plan and Components of Prefabricated Building with Energy-Cost Effect},
  author = {Guo, Juanli and Li, Mingchen and Jiang, Zixin and Wang, Zhoupeng and Zhou, Yangkong},
  year = {2022},

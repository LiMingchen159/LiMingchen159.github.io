  title = {A Simulation-Based Optimization Method for Cost-Optimal Analysis of Nearly {{Zero Energy Buildings}}},
  author = {Ferrara, Maria and Fabrizio, Enrico and Virgone, Joseph and Filippi, Marco},
  year = {2014},

@inproceedings{blankInvestigatingNormalizationProcedure2019a,
  title = {Investigating the {{Normalization Procedure}} of {{NSGA-III}}},
  booktitle = {Evolutionary {{Multi-Criterion Optimization}}},
  author = {Blank, Julian and Deb, Kalyanmoy and Roy, Proteek Chandan},
  editor = {Deb, Kalyanmoy and Goodman, Erik and Coello Coello, Carlos A. and Klamroth, Kathrin and Miettinen, Kaisa and Mostaghim, Sanaz and Reed, Patrick},

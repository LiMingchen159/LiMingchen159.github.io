@inproceedings{fierroShepherdingMetadataBuilding2020,
  title = {Shepherding {{Metadata Through}} the {{Building Lifecycle}}},
  booktitle = {Proceedings of the 7th {{ACM International Conference}} on {{Systems}} for {{Energy-Efficient Buildings}}, {{Cities}}, and {{Transportation}}},
  author = {Fierro, Gabe and Prakash, Anand Krishnan and Mosiman, Cory and Pritoni, Marco and Raftery, Paul and Wetter, Michael and Culler, David E.},
  year = {2020},
  month = nov,
  pages = {70--79},
  publisher = {ACM},
  address = {Virtual Event Japan},
  doi = {10.1145/3408308.3427627},
  urldate = {2024-02-17},
  abstract = {Many different digital representations of a building are produced over the course of its lifecycle. These representations contain the metadata required to support different stages of the building, from initial planning and design, to construction and commissioning, through operations, audits, retrofits and maintenance. However, because of differences in the semantics, structure and syntax of these representations, the metadata they contain is not interoperable. We present a novel method for leveraging these representations to create a unified, authoritative Brick metadata model for a building that can be continually maintained over the course of the building lifecycle. A simple synchronization protocol relays inferred Brick metadata from existing metadata sources such as gbXML, BuildingSync, Project Haystack and Modelica to a central integration server, which merges the metadata into a valid Brick model.},
  isbn = {978-1-4503-8061-4},
  langid = {english},
  keywords = {/unread},
  annotation = {14 citations (Crossref/DOI) [2024-06-27]},
  file = {C\:\\Users\\liwei\\OneDrive\\Zotero\\Multi-Task-Learning-Paper\\Fierro et al. - 2020 - Shepherding Metadata Through the Building Lifecycle_1.pdf;C\:\\Users\\liwei\\OneDrive\\Zotero\\Multi-Task-Learning-Paper\\Fierro et al. - 2020 - Shepherding Metadata Through the Building Lifecycle.pdf}
}


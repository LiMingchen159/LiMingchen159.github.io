  title = {A Systematic Comparison and Evaluation of Building Ontologies for Deploying Data-Driven Analytics in Smart Buildings},
  author = {Qiang, Zhangcheng and Hands, Stuart and Taylor, Kerry and Sethuvenkatraman, Subbu and Hugo, Daniel and Ghiasnezhad Omran, Pouya and Perera, Madhawa and Haller, Armin},
  year = {2023},

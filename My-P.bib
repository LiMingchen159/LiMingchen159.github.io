  title = {Review of Current Status, Requirements and Opportunities for Building Performance Simulation of Adaptive Facades},
  author = {Loonen, Roel C.G.M. and Favoino, Fabio and Hensen, Jan L.M. and Overend, Mauro},
  year = {2017},
  month = mar,

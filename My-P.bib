@article{panClusterAnalysisOccupantbehavior2017,
  title = {Cluster Analysis for Occupant-Behavior Based Electricity Load Patterns in Buildings: {{A}} Case Study in {{Shanghai}} Residences},
  shorttitle = {Cluster Analysis for Occupant-Behavior Based Electricity Load Patterns in Buildings},
  author = {Pan, Song and Wang, Xinru and Wei, Yixuan and Zhang, Xingxing and Gal, Csilla and Ren, Guangying and Yan, Da and Shi, Yong and Wu, Jinshun and Xia, Liang and Xie, Jingchao and Liu, Jiaping},
  year = {2017},
